    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" prefHeight="34.0" prefWidth="282.0" text="Tic-Tac-Toe" />
   <Button mnemonicParsing="false" prefHeight="33.0" prefWidth="282.0" text="Minesweeper" />
   <Button mnemonicParsing="false" prefHeight="34.0" prefWidth="283.0" text="Settings" />
   <Button mnemonicParsing="false" prefHeight="31.0" prefWidth="283.0" text="Exit" />
</VBox>
